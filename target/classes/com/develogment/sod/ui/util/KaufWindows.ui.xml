<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<?xdevuiml version="1.0"?>
<?template BROWSER_XGA?>
<?import com.develogment.sod.ui.second.Cat2MainContainer?>
<?import com.vaadin.flow.component.html.Div?>
<?import com.vaadin.flow.component.html.Label?>
<?import com.vaadin.flow.component.orderedlayout.HorizontalLayout?>
<?import com.vaadin.flow.component.orderedlayout.VerticalLayout?>
<VerticalLayout xmlns:x="http://xml.xdev-software.com/xdevuiml" x:routeLayout="Cat2MainContainer" x:name="KaufWindows" x:scrollbars="com.rapidclipse.vaadin.editor.model.ScrollbarsPolicy.VERTICAL" x:route="Kaufen">
    <x:constraints zpos="0" width="100%" height="100%" />
    <HorizontalLayout x:name="horizontalLayout">
        <x:constraints width="100px" height="100px" flex-grow="0.0" />
    </HorizontalLayout>
    <VerticalLayout x:name="verticalLayout">
        <x:constraints width="100%" height="-1px" flex-grow="0.0" />
        <Label x:name="label" text="Get more  details by clicking on the  picture">
            <x:constraints width="0px" alignment="STRETCH" height="0px" flex-grow="1.0" />
        </Label>
        <Div x:name="imagelayout">
            <x:constraints width="100%" alignment="STRETCH" height="-1px" flex-grow="0.0" />
        </Div>
        <Div x:name="labelDiv">
            <x:constraints width="100%" alignment="STRETCH" height="-1px" flex-grow="0.0" />
        </Div>
    </VerticalLayout>
</VerticalLayout>
